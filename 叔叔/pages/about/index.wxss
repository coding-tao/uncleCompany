page {
  width: 100%;
  height: 100%;
  background-color: #fff;
  overflow-x: hidden;
}

image{
    width: 90%;
    margin:10rpx 0 0 5%;

}

.container{
    width:100%;
    padding-bottom: 30rpx;
}

.about-wrap{
  background-color: #fff;
  font-size: 30rpx;
  color: #333;
}

.about-wrap .info{
  text-indent: 30rpx;
  margin: 20rpx 5% 20rpx;
  line-height: 50rpx;
}

.tel{
  width: 360rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin:30rpx auto;
  font-size: 24rpx;
  background-color: #42D0DE;
  color: #fff;
  border-radius: 12rpx;
}

.tel.tel1{
    background-color: #F5856F;
}